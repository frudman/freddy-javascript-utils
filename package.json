{
  "name": "tidbits",
  "version": "1.0.11",
  "description": "a library of useful (?) \"out-of-the-beaten-path\" tidbits :-)",
  "main": "lib/umd/es6/index.min.js",
  "module": "utils.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rm -rf lib && webpack --config webpack.config.babel.js",
    "pub": "npm run build && git add . && git commit -m 'working code' && npm version patch -m 'upgrade to %s while working code' && git push && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frudman/freddy-javascript-utils.git"
  },
  "keywords": [
    "utils",
    "helpers",
    "utilities"
  ],
  "author": "Frederic Rudman",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/frudman/freddy-javascript-utils/issues"
  },
  "homepage": "https://github.com/frudman/freddy-javascript-utils#readme",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/register": "^7.0.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "transform-es2015-modules-commonjs"
    ]
  }
}
